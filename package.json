{
    "name": "@aramazanov/eslint-config",
    "version": "2.0.1",
    "description": "Eslint config for corporate usage",
    "files": [
        "/dist",
        "package.json",
        "tsconfig.json",
        "README.md"
    ],
    "keywords": [
        "eslint",
        "eslint config",
        "node",
        "react",
        "typescript",
        "react-typescript"
    ],
    "exports": {
        ".": "./dist/node.js",
        "./node": "./dist/node.js",
        "./react": "./dist/react.js"
    },
    "author": "Almaz Ramazanov",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/PerishableQ/eslint-config.git"
    },
    "homepage": "https://github.com/PerishableQ/eslint-config",
    "scripts": {
        "build": "tsc",
        "version:major": "yarn version major",
        "version:minor": "yarn version minor",
        "version:patch": "yarn version patch",
        "publish:major": "npm run build&&npm run version:major&&npm publish --access public",
        "publish:minor": "npm run build&&npm run version:minor&&npm publish --access public",
        "publish:patch": "npm run build&&npm run version:patch&&npm publish --access public"
    },
    "peerDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.59.8",
        "@typescript-eslint/parser": "^5.59.8",
        "eslint": "^7.0.0 || ^8.0.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-sonarjs": "^0.17.0"
    },
    "dependencies": {
        "@typescript-eslint/eslint-plugin": "^5.59.8",
        "@typescript-eslint/parser": "^5.59.8",
        "eslint": "^7.0.0 || ^8.0.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-sonarjs": "^0.17.0"
    },
    "devDependencies": {
        "@types/eslint": "^8.21.1",
        "@types/node": "^18.14.0",
        "eslint": "^8.34.0",
        "tsconfigs": "^5.0.0",
        "typescript": "^4.9.5"
    }
}
